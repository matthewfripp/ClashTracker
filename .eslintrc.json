{
  "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
      "linebreak-style": 0,
      "indent": 0,
      "arrow-parens": 0,
      "no-restricted-syntax": 0,
      "consistent-return": 0,
      "import/no-dynamic-require": 0,
      "global-require": 0,
      "no-console": 0,
      "no-param-reassign": 0,
      "max-len": 0,
      "no-await-in-loop": 0,
      "no-plusplus": 0,
      "prefer-template": 0,
      "no-continue": 0,
      "no-underscore-dangle": 0,
      "class-methods-use-this": 0
  }
}
